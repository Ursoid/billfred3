[account]
jid=
password=
room=
room_password=
nick=

[database]
database_path=


# Logging config
[loggers]
keys = root,billfred,sleekxmpp

[handlers]
keys = console
# Enable additional handlers like this
# keys = console,mail,file

[formatters]
keys = generic

[logger_root]
level = INFO
handlers = console

# Settings for sleekxmpp
[logger_sleekxmpp]
level = INFO
qualname = sleekxmpp
handlers = console
propagate = 0

[logger_billfred]
level = DEBUG
qualname = billfred
handlers = console
# handlers = file, mail
propagate = 0

[formatter_generic]
format = %(asctime)s %(levelname)s [%(name)s] %(message)s
datefmt = %Y-%m-%d %H:%M:%S

[handler_console]
class = StreamHandler
args = (sys.stderr,)
formatter = generic

[rss]
#rss url +  update inerval  in seconds (0 manual update)
#feed0=https://news.google.com/?output=rss 600
#feed1=

